{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@App": ["./App"],
      "@Data/*": ["./App/Data/*"],
      "@Storage/*": ["./App/Data/Storage/*"],
      "@Domain/*": ["./App/Domain/*"],
      "@Network/*": ["./App/Network/*"],
      "@Helper/*": ["./App/Helper/*"],
      "@Presentation/*": ["./App/Presentation/*"],
      "@Components/*": ["./App/presentation/components/*"]
    }
  },
  "typeRoots": ["./types"]
}
